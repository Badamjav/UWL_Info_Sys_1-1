--- CREATE ORGANISATION TABLE
CREATE TABLE Organisation (
    
Org_ID  NUMBER(6),
Org_name        VARCHAR2(25) NOT NULL,
Org_Description VARCHAR2(25) NOT NULL,
Phone_Number    NUMBER(11)   NOT NULL,
Staff_ID        NUMBER(6)    NOT NULL  

);

