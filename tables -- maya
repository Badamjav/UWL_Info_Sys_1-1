--CREATE STUDENT TABLE
CREATE TABLE Student (
    
Student_ID  NUMBER(6),
Student_name    VARCHAR2(25) NOT NULL,
Sur_Name        VARCHAR2(25) NOT NULL,
Phone_Number    NUMBER(11)   NOT NULL
);

